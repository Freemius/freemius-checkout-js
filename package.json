{
  "name": "freemius-checkout-js",
  "description": "Freemius Checkout JS, without jQuery or other dependencies, written in TypeScript as ES Modules.",
  "version": "1.3.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "build:lib": "tsup",
    "deploysite": "npm run build && netlify deploy --dir=dist --prod",
    "prepublish": "npm run build:lib",
    "test": "jest",
    "prepare": "ts-patch install -s"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^3.3.3",
    "ts-auto-mock": "^3.7.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "license": "MIT",
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "main": "./lib/checkout.js",
  "module": "./lib/checkout.mjs",
  "exports": {
    ".": {
      "import": "./lib/checkout.mjs",
      "require": "./lib/checkout.js"
    }
  },
  "types": "./lib/checkout.d.ts",
  "author": {
    "name": "Swashata Ghosh",
    "email": "swashata4u@gmail.com",
    "url": "https://www.wpeform.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swashata/freemius-checkout-js.git"
  },
  "keywords": [
    "freemius",
    "wordpress",
    "checkout"
  ],
  "homepage": "https://fs-checkout-js.netlify.app/",
  "bugs": "https://github.com/swashata/freemius-checkout-js/issues",
  "type": "module"
}
